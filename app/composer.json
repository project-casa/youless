    "phpunit/php-code-coverage": "^6.1",
    "roeldev/phpcs-ruleset": "^1.1",
    "symfony/var-dumper": "^4.3"
},
  "autoload": {
    "psr-4": {
      "Casa\\YouLess\\": "src/"
