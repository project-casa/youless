{
  "name": "project-casa/youless-logger",
  "description": "Logger service for YouLess energy monitor",
  "license": "GPL-3.0+",
  "homepage": "https://github.com/project-casa/youless-logger",
  "authors": [
    {
      "name": "Roel Schut",
      "homepage": "https://roelschut.nl"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "stellar/stellar": "~0.4@dev",
    "symfony/console": "^4.3",
    "symfony/http-foundation": "^4.3",
    "symfony/routing": "^4.3"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.19",
    "fzaninotto/faker": "^1.8",
    "nunomaduro/collision": "^2",
